{
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": [
                "over-block.js"
            ],
            "run_at": "document_idle"
        }
    ],
    "description": "This extension removes annoying ad popovers from your browsing experience.",
    "manifest_version": 2,
    "name": "Popover Ad Block",
    "permissions": [
        "activeTab"
    ],
    "version": "1.0"
}
